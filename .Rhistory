mvnpdf <- function(X, mean, varcovM, Log = TRUE){
p <- nrow(X)
X0 <- sweep(X, 2, mean, "-")
density <- diag((2*pi)^(-p/2)*det(varcovM)^(-0.5)*exp(-0.5*t(X0) %*% solve(varcovM) %*% X0))
if (Log){
density <- log(density)
}
output <- list("Input matrix" = X, "MVN density" = density)
return(output)
}
mvnpdf(X, mean, varcovM, Log = TRUE)
